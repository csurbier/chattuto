{
	"appId": "com.idevotion.chattuto",
	"appName": "ChatTuto",
	"webDir": "www",
	"bundledWebRuntime": false,
	"plugins": {
		"SplashScreen": {
			"launchAutoHide": false,
			"androidScaleType": "CENTER_CROP",
			"androidSpinnerStyle": "large",
			"iosSpinnerStyle": "large",
			"spinnerColor": "#ffffff",
			"showSpinner": true,
			"splashFullScreen": true,
			"splashImmersive": true
		},
		"PushNotifications": {
			"presentationOptions": [
				"badge",
				"sound",
				"alert"
			]
		}
	}
}
